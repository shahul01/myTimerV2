
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // url      = "db.sqlite"
  url      = env("DATABASE_URL")
}

model Task {
// id          TEXT PRIMARY KEY DESC
  id           String   @id @unique @default(uuid())
  title        String
  timerInput   String
  currentTimer String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model Example {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
