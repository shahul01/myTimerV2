
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  // url      = "db.sqlite"
  url      = env("DATABASE_URL")
}

model Example {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Task {
// id          TEXT PRIMARY KEY DESC
  id           Int         @id @unique
  title        String
  timerInput   String
  currentTimer String
  // createdAt    DateTime @default(now())
  // updatedAt    DateTime @updatedAt
}

model LogByDate {
  id String @id @unique @default(uuid())
  date DateTime
  taskName String
  timeSpent String
}
